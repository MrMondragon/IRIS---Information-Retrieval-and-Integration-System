<?xml version="1.0" encoding="utf-8"?>
<LexScheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="scheme.xsd">
  <Author>Quantum Whale</Author>
  <Name>C++Builder language</Name>
  <Desc>Syntax Scheme for C++ Builder Language</Desc>
  <Copyright>Copyright Quantum Whale, 2003</Copyright>
  <FileExtension>.cpp</FileExtension>
  <FileType>cpp</FileType>
  <Version>1.1</Version>
  <Styles>
    <Style>
      <Name>idents</Name>
      <ForeColor>ControlText</ForeColor>
      <Index>0</Index>
    </Style>
    <Style>
      <Name>numbers</Name>
      <ForeColor>ControlText</ForeColor>
      <Index>1</Index>
    </Style>
    <Style>
      <Name>reswords</Name>
      <ForeColor>Blue</ForeColor>
      <Index>2</Index>
    </Style>
    <Style>
      <Name>comments</Name>
      <ForeColor>Green</ForeColor>
      <Index>3</Index>
      <PlainText>true</PlainText>
    </Style>
    <Style>
      <Name>whitespace</Name>
      <ForeColor>WindowText</ForeColor>
      <Index>4</Index>
    </Style>
    <Style>
      <Name>strings</Name>
      <ForeColor>ControlText</ForeColor>
      <Index>5</Index>
    </Style>
    <Style>
      <Name>directives</Name>
      <ForeColor>Blue</ForeColor>
      <Index>6</Index>
    </Style>
  </Styles>
  <States>
    <State>
      <Name>normal</Name>
      <CaseSensitive>true</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>directive</Name>
          <LexStyle>6</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\#.*</string>
          </Expressions>
          <Index>0</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>idents</Name>
          <LexStyle>0</LexStyle>
          <LeaveState>0</LeaveState>
          <ResWordSets>
            <ResWordSet>
              <Name />
              <Index>0</Index>
              <ResWordStyle>2</ResWordStyle>
              <ResWords>
                <word>__int64</word>
                <word>_export</word>
                <word>_cdecl</word>
                <word>do</word>
                <word>__asm</word>
                <word>__stdcall</word>
                <word>else</word>
                <word>inline</word>
                <word>static_cast</word>
                <word>sizeof</word>
                <word>typeid</word>
                <word>__export</word>
                <word>return</word>
                <word>volatile</word>
                <word>typedef</word>
                <word>_fastcall</word>
                <word>__int8</word>
                <word>operator</word>
                <word>float</word>
                <word>continue</word>
                <word>__closure</word>
                <word>__classid</word>
                <word>try</word>
                <word>dynamic_cast</word>
                <word>__pascal</word>
                <word>reinterpret_cast</word>
                <word>char</word>
                <word>enum</word>
                <word>if</word>
                <word>bool</word>
                <word>wchar_t</word>
                <word>__rtti</word>
                <word>asm</word>
                <word>static</word>
                <word>virtual</word>
                <word>_stdcall</word>
                <word>__finally</word>
                <word>switch</word>
                <word>const</word>
                <word>case</word>
                <word>mutable</word>
                <word>__automated</word>
                <word>__dispid</word>
                <word>delete</word>
                <word>union</word>
                <word>__try</word>
                <word>this</word>
                <word>goto</word>
                <word>class</word>
                <word>const_cast</word>
                <word>catch</word>
                <word>extern</word>
                <word>_pascal</word>
                <word>default</word>
                <word>short</word>
                <word>__int16</word>
                <word>cdecl</word>
                <word>_import</word>
                <word>__int32</word>
                <word>public</word>
                <word>int</word>
                <word>__thread</word>
                <word>using</word>
                <word>throw</word>
                <word>template</word>
                <word>__except</word>
                <word>signed</word>
                <word>__declspec</word>
                <word>typename</word>
                <word>private</word>
                <word>__property</word>
                <word>pascal</word>
                <word>friend</word>
                <word>auto</word>
                <word>new</word>
                <word>break</word>
                <word>unsigned</word>
                <word>explicit</word>
                <word>__fastcall</word>
                <word>_asm</word>
                <word>__cdecl</word>
                <word>protected</word>
                <word>long</word>
                <word>true</word>
                <word>while</word>
                <word>void</word>
                <word>false</word>
                <word>struct</word>
                <word>namespace</word>
                <word>for</word>
                <word>double</word>
                <word>register</word>
                <word>__published</word>
                <word>__import</word>
              </ResWords>
            </ResWordSet>
          </ResWordSets>
          <Expressions>
            <string>[a-zA-Z_][a-zA-Z0-9_]*</string>
          </Expressions>
          <Index>1</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comment1</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>//.*</string>
          </Expressions>
          <Index>2</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comment2</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>1</LeaveState>
          <Expressions>
            <string>/\*</string>
          </Expressions>
          <Index>3</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>numbers</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>([0-9]+\.[0-9]*(e|E)(\+|\-)?[0-9]+)|([0-9]+\.[0-9]*)|(0(x|X)[0-9a-fA-F]+)|([0-9]+)</string>
          </Expressions>
          <Index>4</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>string1</Name>
          <LexStyle>5</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>('')|'((((\\')|('')|[^'])*')|(((\\')|('')|[^'])*))</string>
          </Expressions>
          <Index>5</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>string2</Name>
          <LexStyle>5</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>("")|"((((\\")|("")|[^"])*")|(((\\")|("")|[^"])*))</string>
          </Expressions>
          <Index>6</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>string3</Name>
          <LexStyle>5</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>(L"")|{L"}((((\\")|("")|[^"])*")|(((\\")|("")|[^"])*))</string>
          </Expressions>
          <Index>7</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>whitespace</Name>
          <LexStyle>4</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>(\s)*</string>
          </Expressions>
          <Index>8</Index>
        </SyntaxBlock>
      </SyntaxBlocks>
      <Index>0</Index>
    </State>
    <State>
      <Name>comment</Name>
      <CaseSensitive>true</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>comment21</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\*/</string>
          </Expressions>
          <Index>0</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comment22</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>1</LeaveState>
          <Expressions>
            <string>((?!\*/).)+</string>
          </Expressions>
          <Index>1</Index>
        </SyntaxBlock>
      </SyntaxBlocks>
      <Index>1</Index>
    </State>
  </States>
</LexScheme>
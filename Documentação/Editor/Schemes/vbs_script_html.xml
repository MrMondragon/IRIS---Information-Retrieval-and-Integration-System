<?xml version="1.0" encoding="utf-8"?>
<LexScheme xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Author>Quantum Whale</Author>
  <Name>VB script in HTML</Name>
  <Desc>Syntax Scheme for VB Script in HTML Language</Desc>
  <Copyright>Copyright Quantum Whale, 2003</Copyright>
  <FileExtension>.html;htm</FileExtension>
  <FileType>html(vb script)</FileType>
  <Version>1.1</Version>
  <Styles>
    <Style>
      <Name>idents</Name>
      <ForeColor>ControlText</ForeColor>
      <Index>0</Index>
    </Style>
    <Style>
      <Name>numbers</Name>
      <ForeColor>ControlText</ForeColor>
      <Index>1</Index>
    </Style>
    <Style>
      <Name>reswords</Name>
      <ForeColor>Blue</ForeColor>
      <Index>2</Index>
    </Style>
    <Style>
      <Name>comments</Name>
      <ForeColor>Navy</ForeColor>
      <FontStyle>Italic</FontStyle>
      <Index>3</Index>
      <PlainText>true</PlainText>
    </Style>
    <Style>
      <Name>whitespace</Name>
      <ForeColor>0</ForeColor>
      <Index>4</Index>
    </Style>
    <Style>
      <Name>strings</Name>
      <ForeColor>Maroon</ForeColor>
      <Index>5</Index>
    </Style>
    <Style>
      <Name>reswords1</Name>
      <ForeColor>Teal</ForeColor>
      <Index>6</Index>
    </Style>
  </Styles>
  <States>
    <State>
      <Name>normal</Name>
      <CaseSensitive>false</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>whitespace</Name>
          <LexStyle>4</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>(\s)+</string>
          </Expressions>
          <Index>0</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>htmlVBS</Name>
          <LexStyle>2</LexStyle>
          <LeaveState>2</LeaveState>
          <Expressions>
            <string>&lt;script\slanguage="vbscript"</string>
          </Expressions>
          <Index>1</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>common</Name>
          <LexStyle>2</LexStyle>
          <LeaveState>1</LeaveState>
          <Expressions>
            <string>&lt;</string>
          </Expressions>
          <Index>2</Index>
        </SyntaxBlock>
      </SyntaxBlocks>
      <Index>0</Index>
    </State>
    <State>
      <Name>htmltag</Name>
      <CaseSensitive>true</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>common21</Name>
          <LexStyle>2</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>&gt;</string>
          </Expressions>
          <Index>0</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>common22</Name>
          <LexStyle>2</LexStyle>
          <LeaveState>1</LeaveState>
          <Expressions>
            <string>[a-zA-Z_][a-zA-Z0-9_]*</string>
          </Expressions>
          <Index>1</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>numbers</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>1</LeaveState>
          <Expressions>
            <string>([0-9]+\.[0-9]*(e|E)(\+|\-)?[0-9]+)|([0-9]+\.[0-9]*)|(\#[0-9a-fA-F]+)|([0-9]+)</string>
          </Expressions>
          <Index>2</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>strings</Name>
          <LexStyle>5</LexStyle>
          <LeaveState>1</LeaveState>
          <Expressions>
            <string>("")|"((((\\")|("")|[^"])*")|(((\\")|("")|[^"])*))</string>
            <string>('')|'((((\\')|('')|[^'])*')|(((\\')|('')|[^'])*))</string>
          </Expressions>
          <Index>3</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comments</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>3</LeaveState>
          <Expressions>
            <string>&lt;\!--</string>
          </Expressions>
          <Index>4</Index>
        </SyntaxBlock>
      </SyntaxBlocks>
      <Index>1</Index>
    </State>
    <State>
      <Name>htmlVBS</Name>
      <CaseSensitive>false</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>idents</Name>
          <LexStyle>0</LexStyle>
          <LeaveState>2</LeaveState>
          <ResWordSets>
            <ResWordSet>
              <Name />
              <Index>0</Index>
              <ResWordStyle>6</ResWordStyle>
              <ResWords>
                <word>Private</word>
                <word>end</word>
                <word>Empty</word>
                <word>Const</word>
                <word>Select</word>
                <word>While</word>
                <word>Type</word>
                <word>Compare</word>
                <word>get</word>
                <word>Or</word>
                <word>Next</word>
                <word>Wend</word>
                <word>Option</word>
                <word>Mod</word>
                <word>Is</word>
                <word>Then</word>
                <word>Nothing</word>
                <word>friend</word>
                <word>Case</word>
                <word>Attribute</word>
                <word>Function</word>
                <word>ReDim</word>
                <word>On</word>
                <word>Exit</word>
                <word>If</word>
                <word>Base</word>
                <word>Not</word>
                <word>False</word>
                <word>Call</word>
                <word>String</word>
                <word>let</word>
                <word>Loop</word>
                <word>Each</word>
                <word>To</word>
                <word>Declare</word>
                <word>and</word>
                <word>Xor</word>
                <word>property</word>
                <word>Else</word>
                <word>As</word>
                <word>Date</word>
                <word>With</word>
                <word>Error</word>
                <word>Set</word>
                <word>Explicit'</word>
                <word>Elseif</word>
                <word>Dim</word>
                <word>True</word>
                <word>Sub</word>
                <word>ByVal</word>
                <word>Public</word>
                <word>For</word>
                <word>Do</word>
                <word>Null</word>
              </ResWords>
            </ResWordSet>
          </ResWordSets>
          <Expressions>
            <string>[a-zA-Z_][a-zA-Z0-9_]*</string>
          </Expressions>
          <Index>0</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>numbers</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>2</LeaveState>
          <Expressions>
            <string>([0-9]+\.[0-9]*(e|E)(\+|\-)?[0-9]+)|([0-9]+\.[0-9]*)|([0-9]+)</string>
          </Expressions>
          <Index>1</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>strings</Name>
          <LexStyle>5</LexStyle>
          <LeaveState>2</LeaveState>
          <Expressions>
            <string>("")|"((((\\")|("")|[^"])*")|(((\\")|("")|[^"])*))</string>
          </Expressions>
          <Index>2</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comments</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>2</LeaveState>
          <Expressions>
            <string>\'.*</string>
            <string>rem[\u0000-\u0020].*</string>
          </Expressions>
          <Index>3</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>htmlVBS</Name>
          <LexStyle>2</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>&lt;/script&gt;</string>
          </Expressions>
          <Index>4</Index>
        </SyntaxBlock>
      </SyntaxBlocks>
      <Index>2</Index>
    </State>
    <State>
      <Name>comment1</Name>
      <CaseSensitive>false</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>comment12</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>1</LeaveState>
          <Expressions>
            <string>--&gt;</string>
          </Expressions>
          <Index>0</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comment22</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>3</LeaveState>
          <Expressions>
            <string>((?!--&gt;).)+</string>
          </Expressions>
          <Index>1</Index>
        </SyntaxBlock>
      </SyntaxBlocks>
      <Index>3</Index>
    </State>
  </States>
</LexScheme>
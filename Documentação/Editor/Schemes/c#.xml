<?xml version="1.0" encoding="utf-8"?>
<LexScheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="scheme.xsd">
  <Author>Quantum	Whale</Author>
  <Name>C# language</Name>
  <Desc>Syntax Scheme for C# Language</Desc>
  <Copyright>Copyright Quantum Whale, 2003</Copyright>
  <FileExtension>.cs</FileExtension>
  <FileType>c#</FileType>
  <Version>1.1</Version>
  <Styles>
    <Style>
      <Name>idents</Name>
      <ForeColor>ControlText</ForeColor>
      <Index>0</Index>
    </Style>
    <Style>
      <Name>numbers</Name>
      <ForeColor>ControlText</ForeColor>
      <Index>1</Index>
    </Style>
    <Style>
      <Name>reswords</Name>
      <ForeColor>Blue</ForeColor>
      <Index>2</Index>
    </Style>
    <Style>
      <Name>comments</Name>
      <ForeColor>Green</ForeColor>
      <Index>3</Index>
      <PlainText>true</PlainText>
    </Style>
    <Style>
      <Name>xml_comments</Name>
      <ForeColor>Silver</ForeColor>
      <Index>4</Index>
      <PlainText>true</PlainText>
    </Style>
    <Style>
      <Name>symbol</Name>
      <ForeColor>Gray</ForeColor>
      <Index>5</Index>
    </Style>
    <Style>
      <Name>whitespace</Name>
      <ForeColor>WindowText</ForeColor>
      <Index>6</Index>
    </Style>
    <Style>
      <Name>strings</Name>
      <ForeColor>ControlText</ForeColor>
      <Index>7</Index>
    </Style>
    <Style>
      <Name>directives</Name>
      <ForeColor>Blue</ForeColor>
      <Index>8</Index>
    </Style>
  </Styles>
  <States>
    <State>
      <Name>normal</Name>
      <CaseSensitive>true</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>directive</Name>
          <LexStyle>0</LexStyle>
          <LeaveState>0</LeaveState>
          <ResWordSets>
            <ResWordSet>
              <Name />
              <Index>0</Index>
              <ResWordStyle>8</ResWordStyle>
              <ResWords>
                <word>#region</word>
                <word>#elif</word>
                <word>#undef</word>
                <word>#endif/word</word>
                <word>#if</word>
                <word>#line</word>
                <word>#warning</word>
                <word>#endregion</word>
                <word>#else</word>
                <word>#error</word>
                <word>#define</word>
              </ResWords>
            </ResWordSet>
          </ResWordSets>
          <Expressions>
            <string>\#[a-zA-Z_][a-zA-Z0-9_]*</string>
          </Expressions>
          <Index>0</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>idents</Name>
          <LexStyle>0</LexStyle>
          <LeaveState>0</LeaveState>
          <ResWordSets>
            <ResWordSet>
              <Name />
              <Index>0</Index>
              <ResWordStyle>2</ResWordStyle>
              <ResWords>
                <word>protected</word>
                <word>else</word>
                <word>foreach</word>
                <word>int</word>
                <word>switch</word>
                <word>operator</word>
                <word>uint</word>
                <word>out</word>
                <word>ushort</word>
                <word>long</word>
                <word>enum</word>
                <word>class</word>
                <word>ref</word>
                <word>char</word>
                <word>false</word>
                <word>this</word>
                <word>sbyte</word>
                <word>throw</word>
                <word>struct</word>
                <word>checked</word>
                <word>event</word>
                <word>private</word>
                <word>explicit</word>
                <word>short</word>
                <word>interface</word>
                <word>const</word>
                <word>float</word>
                <word>using</word>
                <word>while</word>
                <word>new</word>
                <word>for</word>
                <word>override</word>
                <word>base</word>
                <word>continue</word>
                <word>goto</word>
                <word>finally</word>
                <word>is</word>
                <word>ulong</word>
                <word>try</word>
                <word>namespace</word>
                <word>break</word>
                <word>do</word>
                <word>word</word>
                <word>return</word>
                <word>abstract</word>
                <word>case</word>
                <word>void</word>
                <word>unchecked</word>
                <word>lock</word>
                <word>default</word>
                <word>public</word>
                <word>extern</word>
                <word>virtual</word>
                <word>true</word>
                <word>unsafe</word>
                <word>if</word>
                <word>null</word>
                <word>catch</word>
                <word>params</word>
                <word>bool</word>
                <word>typeof</word>
                <word>in</word>
                <word>sealed</word>
                <word>readonly</word>
                <word>byte</word>
                <word>decimal</word>
                <word>static</word>
                <word>object</word>
                <word>sizeof</word>
                <word>internal</word>
                <word>fixed</word>
                <word>implicit</word>
                <word>double</word>
                <word>delegate</word>
              </ResWords>
            </ResWordSet>
          </ResWordSets>
          <Expressions>
            <string>[a-zA-Z_][a-zA-Z0-9_]*</string>
            <string>@[a-zA-Z_][a-zA-Z0-9_]*</string>
          </Expressions>
          <Index>1</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comment1</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>1</LeaveState>
          <Expressions>
            <string>/\*</string>
          </Expressions>
          <Index>2</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comment2</Name>
          <LexStyle>4</LexStyle>
          <LeaveState>2</LeaveState>
          <Expressions>
            <string>///</string>
          </Expressions>
          <Index>3</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comment3</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>//.*</string>
          </Expressions>
          <Index>4</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>numbers</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>([0-9]+\.[0-9]*(e|E)(\+|\-)?[0-9]+)|([0-9]+\.[0-9]*)|(0(x|X)[0-9a-fA-F]+)|([0-9]+)</string>
          </Expressions>
          <Index>5</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>string1</Name>
          <LexStyle>7</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>("")|"((((\\")|("")|[^"])*")|(((\\")|("")|[^"])*))</string>
            <string>@("")|"((((\\")|("")|[^"])*")|(((\\")|("")|[^"])*))</string>
            <string>('')|'((((\\')|('')|[^'])*')|(((\\')|('')|[^'])*))</string>
          </Expressions>
          <Index>6</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>whitespace</Name>
          <LexStyle>6</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>(\s)+</string>
          </Expressions>
          <Index>7</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>symbol</Name>
          <LexStyle>5</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\W</string>
          </Expressions>
          <Index>8</Index>
        </SyntaxBlock>
      </SyntaxBlocks>
      <Index>0</Index>
    </State>
    <State>
      <Name>comment</Name>
      <CaseSensitive>true</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>comment21</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\*/</string>
          </Expressions>
          <Index>0</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comment22</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>1</LeaveState>
          <Expressions>
            <string>((?!\*/).)+</string>
          </Expressions>
          <Index>1</Index>
        </SyntaxBlock>
      </SyntaxBlocks>
      <Index>1</Index>
    </State>
    <State>
      <Name>commentXML</Name>
      <CaseSensitive>true</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>comment31</Name>
          <LexStyle>4</LexStyle>
          <LeaveState>4</LeaveState>
          <Expressions>
            <string>\u003C</string>
          </Expressions>
          <Index>0</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comment32</Name>
          <LexStyle>4</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>$</string>
          </Expressions>
          <Index>1</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comment33</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>3</LeaveState>
          <Expressions>
            <string>[^\u003C]*</string>
          </Expressions>
          <Index>2</Index>
        </SyntaxBlock>
      </SyntaxBlocks>
      <Index>2</Index>
    </State>
    <State>
      <Name>xml_comment</Name>
      <CaseSensitive>true</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>comment41</Name>
          <LexStyle>4</LexStyle>
          <LeaveState>4</LeaveState>
          <Expressions>
            <string>\u003C</string>
          </Expressions>
          <Index>0</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comment42</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>$</string>
          </Expressions>
          <Index>1</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comment43</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>3</LeaveState>
          <Expressions>
            <string>[^\u003C]*</string>
          </Expressions>
          <Index>2</Index>
        </SyntaxBlock>
      </SyntaxBlocks>
      <Index>3</Index>
    </State>
    <State>
      <Name>tag_comment</Name>
      <CaseSensitive>true</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>comment51</Name>
          <LexStyle>4</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>$</string>
          </Expressions>
          <Index>0</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comment52</Name>
          <LexStyle>4</LexStyle>
          <LeaveState>3</LeaveState>
          <Expressions>
            <string>\u003E</string>
          </Expressions>
          <Index>1</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comment53</Name>
          <LexStyle>4</LexStyle>
          <LeaveState>4</LeaveState>
          <Expressions>
            <string>[^\u003E]*</string>
          </Expressions>
          <Index>2</Index>
        </SyntaxBlock>
      </SyntaxBlocks>
      <Index>4</Index>
    </State>
  </States>
</LexScheme>
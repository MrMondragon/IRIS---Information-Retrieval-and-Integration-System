  <Name>SQL (Oracle) language</Name>
  <Desc>Syntax Scheme for SQL(Oracle) Language</Desc>
  <Copyright>Copyright Quantum Whale, 2003</Copyright>
  <FileExtension>.sql</FileExtension>
  <FileType>sql(oracle)</FileType>
  <Version>1.1</Version>
  <Styles>
    <Style>
      <Name>idents</Name>
      <ForeColor>ControlText</ForeColor>
      <Index>0</Index>
    </Style>
    <Style>
      <Name>numbers</Name>
      <ForeColor>ControlText</ForeColor>
      <Index>1</Index>
    </Style>
    <Style>
      <Name>reswords</Name>
      <ForeColor>ControlText</ForeColor>
      <FontStyle>Bold</FontStyle>
      <Index>2</Index>
    </Style>
    <Style>

    </Style>
    <Style>
      <Name>whitespace</Name>
      <ForeColor>0</ForeColor>
      <Index>4</Index>
    </Style>
    <Style>
      <Name>strings</Name>
      <ForeColor>Maroon</ForeColor>
      <Index>5</Index>
    </Style>
  </Styles>
